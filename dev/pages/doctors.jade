                    li(data-filter="protez", class="doctors-filter-item")
                        span Протезирование
                    li(data-filter="implant", class="doctors-filter-item")
                        span Имплантация
                    li(data-filter="hirurg", class="doctors-filter-item")
                        span Хирургия
                    li(data-filter="ortodon", class="doctors-filter-item")
